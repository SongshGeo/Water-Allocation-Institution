}

@article{heFactorscontrollingsimilar2015,
  title = {Factors Controlling the Past Similar to 150-Year Ecological Dynamics of {{Lake Wuliangsu}} in the Upper Reaches of the {{Yellow River}}, {{China}}},
  author = {He, Yuxin and Sun, Dayang and Wu, Jinglu and Sun, Yongge},
