@article{snorekDivergentadaptationclimate2014,
  title = {Divergent Adaptation to Climate Variability: {{A}} Case Study of Pastoral and Agricultural Societies in {{Niger}}},
  shorttitle = {Divergent Adaptation to Climate Variability},
  author = {Snorek, Julie and Renaud, Fabrice G. and Kloos, Julia},
