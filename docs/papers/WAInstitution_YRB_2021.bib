@article{wuVariabilitytetraetherlipids2013,
  title = {Variability of Tetraether Lipids in {{Yellow River-dominated}} Continental Margin during the Past Eight Decades: {{Implications}} for Organic Matter Sources and River Channel Shifts},
  author = {Wu, Weichao and Zhao, Liang and Pei, Yandong and Ding, Weihua and Yang, Huan and Xu, Yunping},
  year = {2013},
