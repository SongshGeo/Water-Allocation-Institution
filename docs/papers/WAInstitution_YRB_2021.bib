@article{bodinReconcilingConflictCooperation2020,
  title = {Reconciling {{Conflict}} and {{Cooperation}} in {{Environmental Governance}}: {{A Social Network Perspective}}},
  shorttitle = {Reconciling {{Conflict}} and {{Cooperation}} in {{Environmental Governance}}},
  author = {Bodin, {\"O}rjan and Garc{\'i}a, Mar{\'i}a Mancilla and Robins, Garry},
