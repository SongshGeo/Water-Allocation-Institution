@article{dalinBalancingwaterresource2015,
  title = {Balancing Water Resource Conservation and Food Security in {{China}}},
  author = {Dalin, Carole and Qiu, Huanguang and Hanasaki, Naota and Mauzerall, Denise L. and {Rodriguez-Iturbe}, Ignacio},
