@article{castilla-rhoagentbasedplatformsimulating2015,
  title = {An Agent-Based Platform for Simulating Complex Human\textendash Aquifer Interactions in Managed Groundwater Systems},
  author = {{Castilla-Rho}, J.C. and Mariethoz, G. and Rojas, R. and Andersen, M.S. and Kelly, B.F.J.},
