@article{batistaesilvaUncoveringtemporalchanges2020,
  title = {Uncovering Temporal Changes in {{Europe}}'s Population Density Patterns Using a Data Fusion Approach},
  author = {{Batista e Silva}, Filipe and Freire, S{\'e}rgio and Schiavina, Marcello and Rosina, Kon{\v s}tant{\'i}n and {Mar{\'i}n-Herrera}, Mario Alberto and Ziemba, Lukasz and Craglia, Massimo and Koomen, Eric and Lavalle, Carlo},
