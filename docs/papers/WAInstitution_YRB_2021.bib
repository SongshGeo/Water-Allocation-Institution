@article{sayles2017,
  ids = {saylesSocialecologicalnetworkanalysis2017,saylesSocialecologicalnetworkanalysis2017a},
  title = {Socialâ€“Ecological Network Analysis of Scale Mismatches in Estuary Watershed Restoration},
  author = {Sayles, Jesse S. and Baggio, Jacopo A.},
