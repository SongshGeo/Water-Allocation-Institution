@article{taylorLatestagedeforestationenhances2022,
  title = {``{{Late-stage}}'' Deforestation Enhances Storm Trends in Coastal {{West Africa}}},
  author = {Taylor, Christopher M. and Klein, Cornelia and Parker, Douglas J. and Gerard, France and Semeena, Valiyaveetil Shamsudheen and Barton, Emma J. and Harris, Bethan L.},
