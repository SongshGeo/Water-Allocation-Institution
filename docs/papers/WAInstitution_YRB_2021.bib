@article{mengComplexitybasedapproach2020,
  title = {Complexity Based Approach for {{El Nino}} Magnitude Forecasting before the "Spring Predictability Barrier"},
  author = {Meng, Jun and Fan, Jingfang and Ludescher, Josef and Ankit, Agarwal and Chen, Xiaosong and Bunde, Armin and Kurths, Jurgen and Schellnhuber, Hans Joachim},
