}

@article{wang2019a,
  title = {Review of the Implementation of the Yellow River Water Allocation Scheme for Thirty Years},
  author = {Wang, Yu and Peng, Shaoming and Wu, jian and Ming, Guanghui and Jiang, Guiqin and Fang, Hongbin and Chen, Cuixia},
  date = {2019},
  journaltitle = {Yellow River},
  volume = {41},
