}

@article{steinhaeuserMultivariatemultiscaledependence2012,
  title = {Multivariate and Multiscale Dependence in the Global Climate System Revealed through Complex Networks},
  author = {Steinhaeuser, Karsten and Ganguly, Auroop R. and Chawla, Nitesh V.},
  year = {2012},
