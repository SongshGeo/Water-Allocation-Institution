@article{barnesSocialnetworksenvironmental2016,
  ids = {barnesSocialnetworksenvironmental2016a,barnesSocialnetworksenvironmental2016b},
  title = {Social Networks and Environmental Outcomes},
  author = {Barnes, Michele L. and Lynham, John and Kalberg, Kolter and Leung, PingSun},
