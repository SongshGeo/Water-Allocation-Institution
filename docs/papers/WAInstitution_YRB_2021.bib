@article{yaocontributionecosystemrestoration2021,
  ids = {yaocontributionecosystemrestoration2021a},
  title = {The Contribution of Ecosystem Restoration to Sustainable Development Goals in {{Asian}} Drylands: {{A}} Literature Review},
  shorttitle = {The Contribution of Ecosystem Restoration to Sustainable Development Goals in {{Asian}} Drylands},
  author = {Yao, Ying and Fu, Bojie and Liu, Yanxu and Wang, Yijia and Song, Shuang},
  year = {2021},
  month = aug,
  journal = {Land Degradation \& Development},
  pages = {ldr.4065},
  issn = {1085-3278, 1099-145X},
  doi = {10.1002/ldr.4065},
  abstract = {The multiple effects of ecosystem restoration programmes deserve attention. After reviewing the social-ecological effects of 23 ecosystem restoration programs in Asia's drylands, we find that these programmes mainly contribute to sustainable development goals (SDGs) synergistically, but the trade-offs between social-ecological effects still exist. Among the five goals of SDG15 (Life on Land), SDG13 (Climate Action), SDG6 (Clean Water and Sanitation), SDG1 (No poverty), and SDG2 (Zero Hunger), 11 programs can synergistically achieve no less than three goals, especially grassland restoration and water diversion in China, as well as water management programmes in Israel. However, the contribution of ecosystem restoration programs to SDG15 easily weakens SDG6, SDG1, and SDG2, indicating the competition of land and water between ecosystem restoration and agriculture. To reduce the trade-offs among SGDs caused by ecosystem restoration, we propose the social-ecological system research framework of 'Dryland Boundary \textendash{} Water, Food, Energy and Ecosystem Nexus \textendash{} Meta-coupling \textendash{} Nature-based Solutions' to guide the implementation of ecosystem restoration programmes from four aspects: supply\textendash demand matching, element matching, regional matching, and local adaptation.},
  langid = {english},
  keywords = {üåü ÊòüÊ†á},
  annotation = {SC: None[s1]},
  file = {/Users/songshgeo/Zotero/storage/BALQAHLP/Yao Á≠â„ÄÇ - 2021 - The contribution of ecosystem restoration to susta.pdf;/Users/songshgeo/Zotero/storage/X9U2JLFN/Yao Á≠â„ÄÇ - 2021 - The contribution of ecosystem restoration to susta.pdf}
}

@article{yaoImpactsClimateChange2015,
  title = {Impacts of {{Climate Change}} and {{Human Activities}} on {{Runoff}} and {{Sediment Load}} of the {{Xiliugou Basin}} in the {{Upper Yellow River}}},
  author = {Yao, Haifang and Shi, Changxing and Shao, Wenwei and Bai, Jianbin and Yang, Hui},
