@article{jingoverviewresiliencesocialecological2007,
  title = {An Overview on the Resilience of Social-Ecological Systems},
  author = {Jing, S. U. N. and Jun, WANG and XinJun, YANG},
  year = {2007},
  journal = {Acta Ecologica Sinica},
