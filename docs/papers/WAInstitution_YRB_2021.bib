@article{wang2019d,
  ids = {wangAlignmentSocialEcological2019,wangAlignmentsocialecological2019a},
  title = {Alignment of Social and Ecological Structures Increased the Ability of River Management},
  author = {Wang, Shuai and Fu, Bojie and Bodin, Orjan and Liu, Jianguo and Zhang, Mengmeng and Li, Xiaoyan},
  date = {2019-09-30},
  journaltitle = {Science Bulletin},
