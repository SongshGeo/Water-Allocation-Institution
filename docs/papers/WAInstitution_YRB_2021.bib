@article{luoWhycommoninterests2021,
  title = {Why Common Interests and Collective Action Are Not Enough for Environmental Cooperation \textendash{} {{Lessons}} from the {{China-EU}} Cooperation Discourse on Circular Economy},
  author = {Luo, Anran and Zuberi, Mehwish and Liu, Jiayu and Perrone, Miranda and Schnepf, Simone and Leipold, Sina},
  year = {2021},
  month = nov,
  journal = {Global Environmental Change},
  volume = {71},
  pages = {102389},
  issn = {09593780},
  doi = {10.1016/j.gloenvcha.2021.102389},
  abstract = {Scholars have argued that circular economy (CE) must be upscaled and globalized to address excessive resource extraction and waste generation. Many CE practitioners consider the first international CE agreement between China and the EU a milestone towards such an effort. This analysis shows why this expectation is premature. European and Chinese stakeholders' optimistic and depoliticized CE narratives disable communication regarding development disparities, negative competition and distrust, which shape geopolitical rivalry. While skeptical narratives that address political barriers exist, they fall short of suggesting alternatives. Based on 72 stakeholder interviews, 40 documents and participant observation, these results highlight the critical importance of inves\- tigating narrative strategies to repoliticize environmental cooperation. Such narrative strategies would support contestation and negotiation on development disparities and open channels for joint conceptualization of implementation scales for environmental strategies, circular or otherwise. We therefore propose using constructivist perspectives of international relations to explore discursive barriers and enablers to global envi\- ronmental action.},
  langid = {english},
  keywords = {üìö Êé®Ëçê,üïô ÂæÖÈòÖËØª,collaboration,collaborative governance},
  annotation = {SC: None[s1]},
  file = {/Users/songshgeo/Zotero/storage/GUW6JWD6/Luo Á≠â„ÄÇ - 2021 - Why common interests and collective action are not.pdf}
}

