@article{shiCharacteristicssoilratio2017,
  title = {Characteristics of Soil {{C}}:{{N}} Ratio and Delta {{C-13}} in Wheat-Maize Cropping System of the {{North China Plain}} and Influences of the {{Yellow River}}},
  author = {Shi, Huijin and Wang, Xiujun and Xu, Minggang and Zhang, Haibo and Luo, Yongming},
  year = {2017},
  month = dec,
  journal = {Scientific Reports},
  volume = {7},
  pages = {16854},
  issn = {2045-2322},
  doi = {10.1038/s41598-017-17060-3},
  abstract = {To better understand the characteristics of soil organic matter (SOM) in the North China Plain, we evaluate the large scale variations of soil organic carbon (SOC), total nitrogen (TN), carbon to nitrogen (C:N) ratio and stable carbon isotopic compositions (delta C-13) in SOC over 0-100 cm. To assess the influence of the Yellow River, 31 sites are selected from the wheat-maize double cropping system, and grouped into two: 10 sites near and 21 sites far from the river. Our data show that mean soil C: N ratio is low (7.6-9.9) across the region, and not affected by the Yellow River. However, SOC and TN are significantly (P {$<$} 0.05) lower in subsoil near the Yellow River (2.0 and 0.2-0.3 g kg(-1) for SOC and TN) than far away (3.1 and 0.4 g kg(-1)); delta C-13 is significantly more negative below 60 cm near the river (-23.3 to -22.6\%) than far away (-21.8 to -21.4\%). We estimate that the contributions of wheat and maize to SOC are 61.3-68.1\% and 31.9-38.8\%, respectively. Our analyses indicate that the overall low levels of SOC in the North China Plain may be associated with the low soil C: N ratio and less clay content. The hydrological processes may also partly be responsible, particularly for those near the Yellow River.},
  keywords = {üîç Ê£ÄÁ¥¢},
  annotation = {SC: 0000016}
}

