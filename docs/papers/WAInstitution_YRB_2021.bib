  title = {A Narrative Method for Analyzing Transitions in Urban Water Management: {{The}} Case of the {{Miami-Dade Water}} and {{Sewer Department}}},
  shorttitle = {A Narrative Method for Analyzing Transitions in Urban Water Management},
