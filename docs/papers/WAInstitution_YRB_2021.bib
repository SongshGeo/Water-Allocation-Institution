@article{bierkensShadowPriceIrrigation2019,
  title = {The {{Shadow Price}} of {{Irrigation Water}} in {{Major Groundwater}}-{{Depleting Countries}}},
  author = {Bierkens, Marc F. P. and Reinhard, Stijn and Bruijn, Jens A. and Veninga, Willeke and Wada, Yoshihide},
  year = {2019},
