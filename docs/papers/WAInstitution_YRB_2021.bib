}

@article{liEvolutioncooperationtemporal2020,
  title = {Evolution of Cooperation on Temporal Networks},
  author = {Li, Aming and Zhou, Lei and Su, Qi and Cornelius, Sean P. and Liu, Yang-Yu and Wang, Long and Levin, Simon A.},
  year = {2020},
  month = may,
  journal = {Nature Communications},
  volume = {11},
  number = {1},
  pages = {1--9},
  issn = {2041-1723},
  doi = {10.1038/s41467-020-16088-w},
