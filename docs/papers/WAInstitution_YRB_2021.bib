}

@article{diMultiobjectiveoptimizationwater2021,
  title = {Multi-Objective Optimization for Water Allocation of the {{Yellow River}} Basin Based on Fluid Mechanics, Emergy Theory, and Dynamic Differential Game},
  author = {Di, Danyang and Wu, Zening and Wang, Huiliang and Huang, Shuoqiao},
