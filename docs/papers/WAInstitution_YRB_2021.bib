@article{srinivasanPredictionsociohydrologicalworld2016,
  title = {Prediction in a Socio-Hydrological World},
  author = {Srinivasan, V. and Sanderson, M. and Garcia, M. and Konar, M. and Bl{\"o}schl, G. and Sivapalan, M.},
