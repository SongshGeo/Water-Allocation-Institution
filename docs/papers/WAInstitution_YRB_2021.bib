  author = {Steffen, Will and Richardson, Katherine and Rockstr{\"o}m, Johan and Schellnhuber, Hans Joachim and Dube, Opha Pauline and Dutreuil, S{\'e}bastien and Lenton, Timothy M. and Lubchenco, Jane},
