@article{konarExpandingScopeFoundation2019,
  title = {Expanding the {{Scope}} and {{Foundation}} of {{Sociohydrology}} as the {{Science}} of {{Coupled Human}}-{{Water Systems}}},
  author = {Konar, Megan and Garcia, Margaret and Sanderson, Matthew R. and Yu, David J. and Sivapalan, Murugesu},
