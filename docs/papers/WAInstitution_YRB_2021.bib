@article{fengglobalquantitationfactors,
  title = {A Global Quantitation of Factors Affecting Evapotranspiration Variability},
  author = {Feng, Shuyun and Liu, Jianyu and Zhang, Qiang and Zhang, Yongqiang and Singh, Vijay P. and Gu, Xihui and Sun, Peng},
  journal = {Journal of Hydrology},
  doi = {10.1016/j.jhydrol.2020.124688},
  abstract = {{$<$}p{$>$}Scientific viewpoints on the long-term hydrological responses to factors other than climatic change remain controversial and yet the impacts of these factors are often neglected at the short-term (such as monthly and annual) timescale. We developed an analytical method to decompose evapotranspiration ({$<$}em{$>$}E{$<$}/em{$>$}) variability to the variability of precipitation ({$<$}em{$>$}P{$<$}/em{$>$}), potential evapotranspiration ({$<$}em{$>$}E{$<$}/em{$>$}\textsubscript{0}), total water storage change ({$\Delta<$}em{$>$}S{$<$}/em{$>$}), and catchment characteristics ({$<$}em{$>$}n{$<$}/em{$>$}), such as vegetation, soil and climate seasonality. Global assessment showed that {$<$}em{$>$}P{$<$}/em{$>$} enhanced {$<$}em{$>$}E{$<$}/em{$>$} variability in most regions, while restrained {$<$}em{$>$}E{$<$}/em{$>$} variability in some extremely humid regions. {$<$}em{$>$}E{$<$}/em{$>$}\textsubscript{0} controlled monthly {$<$}em{$>$}E{$<$}/em{$>$} variability in most the energy-constrained regions. {$\Delta<$}em{$>$}S{$<$}/em{$>$} had much larger impacts on {$<$}em{$>$}E{$<$}/em{$>$} variability at monthly scale compared to the annual scale, and restrained {$<$}em{$>$}E{$<$}/em{$>$} variability in many arid regions ({$<$}em{$>$}P{$<$}/em{$>$}/{$<$}em{$>$}E{$<$}/em{$>$}\textsubscript{0}~\&lt;~0.66). Catchment characteristics had larger impacts on {$<$}em{$>$}E{$<$}/em{$>$} variability in humid regions ({$<$}em{$>$}P{$<$}/em{$>$}/{$<$}em{$>$}E{$<$}/em{$>$}\textsubscript{0}~\&gt;~0.66), especially at annual scale. The dominant factors of {$<$}em{$>$}E{$<$}/em{$>$} variability varied with timescales and regions. At a global scale, {$<$}em{$>$}P{$<$}/em{$>$} and catchment characteristics are the dominant factors controlling global {$<$}em{$>$}E{$<$}/em{$>$} variability at monthly and annual scales, respectively; In humid regions, however, the impacts of {$<$}em{$>$}E{$<$}/em{$>$}\textsubscript{0} on monthly {$<$}em{$>$}E{$<$}/em{$>$} variability are generally larger than the impacts of precipitation and catchment characteristics. We highlight the necessity to consider the impacts of catchment characteristics even at the short-term timescales, otherwise the simulation and attribution of {$<$}em{$>$}E{$<$}/em{$>$} variability would be significantly underestimated.{$<$}/p{$>$}},
  keywords = {üíª Êõ¥Êñ∞,Researcher App}
}

