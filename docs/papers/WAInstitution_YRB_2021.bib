}

@article{greveGlobalassessmentwater2018,
  title = {Global Assessment of Water Challenges under Uncertainty in Water Scarcity Projections},
  author = {Greve, P. and Kahil, T. and Mochizuki, J. and Schinko, T. and Satoh, Y. and Burek, P. and Fischer, G. and Tramberend, S. and Burtscher, R. and Langan, S. and Wada, Y.},
