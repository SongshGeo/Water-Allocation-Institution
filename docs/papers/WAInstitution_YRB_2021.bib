@article{fengComparativeAnalysisRunoff2015,
  title = {A {{Comparative Analysis}} of {{Runoff}} and {{Soil Loss Characteristics}} between "{{Extreme Precipitation Year}}" and "{{Normal Precipitation Year}}" at the {{Plot Scale}}: {{A Case Study}} in the {{Loess Plateau}} in {{China}}},
